<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ResellBook.Dao.OrderDao">
    <select id="getStatusOrder" resultType="com.ResellBook.Pojo.Orders" parameterType="java.lang.Integer" >
    SELECT *
    FROM
    orders
    WHERE
    orderStatus=#{num}
    </select>
    <select id="getOrderIdOrder" resultType="com.ResellBook.Pojo.Orders" parameterType="java.lang.Integer">
      SELECT * FROM  orders where  orderId=#{oderid}
    </select >

</mapper>
